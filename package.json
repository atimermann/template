{
  "name": "template",
  "version": "0.0.1",
  "description": "Modelo de projeto javascript (Nodejs ou Fronend), apenas configurações básicas como eslint",
  "main": "src/main.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "fixlint": "eslint --fix .",
    "dev": "node src/main.mjs",
    "docker:build": "docker build -t <IMAGE_NAME>/<IMAGE_NAME> .",
    "docker:run": "docker run --rm -it --name <CONTAINER_NAME> <IMAGE_NAME>/<IMAGE_NAME>",
    "docker:exec": "docker exec -it  <CONTAINER_NAME> bash"
  },
  "repository": {
    "type": "git",
    "url": "CONFIGURAR REPOSITÒRIO GIT"
  },
  "author": "André Timermann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atimermann/template/issues"
  },
  "homepage": "https://github.com/atimermann/template#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0"
  },
  "engines": {
    "node": ">=18"
  }
}
